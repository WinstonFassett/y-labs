{
  "extends": "astro/tsconfigs/strict",
  "exclude": ["node_modules", "dist", ".astro"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ]
  }
}
