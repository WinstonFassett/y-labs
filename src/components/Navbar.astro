---
import {
  Navbar as NavbarContainer,
  NavbarBrand,
  NavbarContent,
} from "@/components/ui/navbar";
const { class: className } = Astro.props;
---

<NavbarContainer className={className}>
  <NavbarBrand>
    <slot name="brand" />
  </NavbarBrand>
  <NavbarContent>
    <slot />
  </NavbarContent>
  <NavbarContent>
    <slot name="end" />
  </NavbarContent>
</NavbarContainer>
