---
import {
  Navbar as NavbarContainer,
  NavbarBrand,
  NavbarContent,
} from "@/components/ui/navbar";
const { class: className } = Astro.props;
---

<NavbarContainer className={className}>
  <NavbarContent>
    <NavbarBrand>
      <slot name="brand" />
    </NavbarBrand>
  </NavbarContent>
  <NavbarContent className="flex-1">
    <slot />
  </NavbarContent>
  <NavbarContent>
    <slot name="end" />
  </NavbarContent>
</NavbarContainer>
